<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
<div id="wrapper"> 
  
  <!-- header --> 
  
  {% include 'header' %}
  <div class="quick-view"></div>
  {% if page == pages.frontpage %} 
  <!-- slider --> 
  {% include 'home-slider' %} 
  
  {% endif %}
  
  
  
  
  
  {% if page == pages.frontpage %}
  <div id="main_content" class="col-main col-sm-12 col-sm-push-3-{{ content_width }}"> {% if template == 'collection' %}
    {% include 'collection.list' %}
    
    {% else %}
    {{ content_for_layout }}
    
    {% endif %} </div>
  {% endif %} 
  
  <!-- main content --> 
  
  {% if template != 'index' %}
  {% if settings.enable_breadcrumbs %}
  <div class="breadcrumbs">{% include 'snippet-breadcrumbs' %}</div>
  {% endif %}
  {% endif %}
  
  
  
  {% unless template == 'index' %}
  <div id="main" class="container" role="main">
    <div class="row"> {% case template %}
      {% when 'index' %}
      {% assign width_sidebars = false %} 
      {% when 'page.homepage' %}
      {% assign width_sidebars = false %} 
      {% when 'page.noasides' %}
      {% assign width_sidebars = false %}
      {% when 'product' %}
      {% assign width_sidebars = false %}
      {% when 'customers/register' %}
      {% assign width_sidebars = false %} 
      {% when 'customers/login' %}
      {% assign width_sidebars = false %}
      {% when 'page.contact' %}
      {% assign width_sidebars = false %}
      {% when 'customers/account' %}
      {% assign width_sidebars = false %}
      {% when 'customers/addresses' %}
      {% assign width_sidebars = false %}
      {% when 'cart' %}
      {% assign width_sidebars = false %}
      {% when 'page' %}
      {% assign width_sidebars = false %}
      {% when 'page.wishlist' %}
      
      {% assign width_sidebars = false %} 
      {% else %}
      {% assign width_sidebars = true %}
      {% endcase %} 
      
      <!-- left column --> 
      
      {% if settings.enable_left_sidebar %}
      {% if width_sidebars == true %}
      <div id="aside-left" class=" col-sm-{{ settings.left_sidebar_width }} column"> {% if blog %}
        {% include 'recent-post' %}
        {% endif %} 
        
        {% if blog %}
        {% else %} 
        {% include 'sidebar-nav' %}
        {% include 'collection-filter' %}
        
        {% endif %} 
        {% include 'widget-types' %}
        
        {% include 'best-seller-single' %}
        {% include 'widget-banner' %}
        {% include 'widget-cms-block' %} </div>
      {% endif %} 
      {% endif %}
      
      
      {% if width_sidebars == true %}
      {% assign content_width = settings.content_width %}
      {% else %}
      {% assign content_width = 12 %}
      {% endif %} 
      
      <!-- center column -->
      <div id="main_content" class="col-main col-sm-{{ content_width }}"> 
        <!-- collection-header --> 
        {% if collection.handle == 'all' %}
        {% include 'collection-header' %} 
        {% endif %}
        {% if template == 'index' %}
        
        {% elsif template == 'list-collections' %}
        
        {% include 'collection.list' %}
        
        {% else%}
        {{ content_for_layout }}
        
        {% endif %} </div>
      
      <!-- right column --> 
      {% if settings.enable_right_sidebar %}
      {% if width_sidebars == true %}
      <div id="aside-right" class="col-sm-{{ settings.right_sidebar_width }} column"> {% if blog %}
        {% include 'recent-post' %}
        {% endif %} 
        
        {% if blog %}
        {% else %} 
        {% include 'sidebar-nav' %}
        {% include 'collection-filter' %}
        {% endif %} 
        {% include 'widget-types' %}
        
        {% include 'best-seller-single' %}
        {% include 'widget-banner' %}
        {% include 'widget-cms-block' %} </div>
      {% endif %}
      {% endif %} </div>
  </div>
  {% endunless %}
  
  {% comment %}Shopify does not support Internet Explorer 6.{% endcomment %} 
